  - key: 2
    dialogue:
    - sentencesData:
      - personName: "\u041F\u0440\u0438\u0437\u0440\u0430\u043A"
        sentence: "\u041E\u043F\u044F\u0442\u044C \u044F \u043D\u0435 \u0441\u043C\u043E\u0433\u043B\u0430
          \u043D\u0438\u043A\u043E\u0433\u043E \u0441\u043F\u0430\u0441\u0442\u0438..."
      - personName: "\u041F\u0440\u0438\u0437\u0440\u0430\u043A"
        sentence: "\u041B\u044E\u0434\u0438... \u0417\u0430\u0447\u0435\u043C \u043E\u043D\u0438
          \u043F\u0440\u0438\u0445\u043E\u0434\u044F\u0442 \u0441\u044E\u0434\u0430?"
      - personName: "\u041F\u0440\u0438\u0437\u0440\u0430\u043A"
        sentence: "\u0425\u0430\u0445... \u0412\u043E\u0442 \u0438 \u043F\u0440\u0438\u0448\u043B\u0430
